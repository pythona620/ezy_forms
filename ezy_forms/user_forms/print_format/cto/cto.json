 "font_size": 14,
 "html": "\r\n<div style=\"font-size: 10pt; margin: 0; padding: 0; font-family: Arial, Helvetica, sans-serif; max-width: 100%;\">\r\n        <table style=\"width: 100%; border-collapse: collapse; border: 1px solid black;\">\r\n        <tr>\r\n            <td style=\"width: 30%; border: 1px solid black;\"></td>\r\n            <td style=\"width: 40%; border: 1px solid black; text-align: center;\">\r\n                <img src=\"/files/company.png\" alt=\"Hyatt Regency\" style=\"height: 50px;\">\r\n            </td>\r\n            <td style=\"width: 30%; border: 1px solid black;\"></td>\r\n        </tr>\r\n        <tr>\r\n            <td style=\"border: 1px solid black;\"></td>\r\n            <td style=\"border: 1px solid black; text-align: center; font-weight: bold;\">Hyatt Regency Chandigarh</td>\r\n            <td style=\"border: 1px solid black; text-align: right; padding-right: 5px;\">Date: {{ frappe.utils.formatdate(frappe.utils.today()) }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"3\" style=\"border: 1px solid black; text-align: center; font-weight: bold;\">COMPARATIVE SHEET CUM SANCTION NOTE</td>\r\n        </tr>\r\n    </table>\r\n    <!-- Header Table -->\r\n\r\n   \r\n\r\n{% set vendors = doc.vendor_details %}\r\n{% set items = doc.ezy_item_details %}\r\n{% import json as js %}\r\n\r\n<table style=\"width: 100%; border-collapse: collapse; border: 1px solid black; margin-top: 10px;\">\r\n    <thead>\r\n        <tr style=\"background-color: #fff7d6;\">\r\n            <th style=\"border: 1px solid black; padding: 3px;\">Sr</th>\r\n            <th style=\"border: 1px solid black; padding: 3px;\">Item Name</th>\r\n            <th style=\"border: 1px solid black; padding: 3px;\">UOM</th>\r\n            <th style=\"border: 1px solid black; padding: 3px;\">Qty</th>\r\n            {% for v in vendors %}\r\n                <th colspan=\"2\" style=\"border: 1px solid black; padding: 3px; text-align: center;\">{{ v.vendor_name }}</th>\r\n            {% endfor %}\r\n        </tr>\r\n        <tr style=\"background-color: #fff7d6;\">\r\n            <th colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\"></th>\r\n            {% for v in vendors %}\r\n                <th style=\"border: 1px solid black; padding: 3px; text-align: center;\">Rate</th>\r\n                <th style=\"border: 1px solid black; padding: 3px; text-align: center;\">Total</th>\r\n            {% endfor %}\r\n        </tr>\r\n        \r\n    </thead>\r\n    <tbody>\r\n        {% for item in items %}\r\n        <tr>\r\n            <td style=\"border: 1px solid black; padding: 3px; text-align: center;\">{{ item.idx }}</td>\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ item.item_name }}</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; text-align: center;\">{{ item.item_unit_of_measure }}</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; text-align: center;\">{{ item.item_quantity }}</td>\r\n\r\n            {% for vendor in vendors %}\r\n            \r\n                {% set matched = None %}\r\n                {% set json_object = js.load(vendor.pricing_details) %}\r\n                {% for p in vendor.pricing_details %}\r\n                    {% if p.item_name == item.item_name %}\r\n                        {% set matched = p %}\r\n                    {% endif %}\r\n                {% endfor %}\r\n\r\n                {% if matched %}\r\n                    <td style=\"border: 1px solid black; padding: 3px; text-align: right;\">{{ matched.unitPrice }}</td>\r\n                    <td style=\"border: 1px solid black; padding: 3px; text-align: right;\">{{ matched.totalPrice }}</td>\r\n                {% else %}\r\n                    <td colspan=\"2\" style=\"border: 1px solid black; padding: 3px; text-align: center;\">N/A</td>\r\n                {% endif %}\r\n            {% endfor %}\r\n        </tr>\r\n        {% endfor %}\r\n        <tr>\r\n            <td colspan=\"3\" style=\"border: 1px solid black; padding: 3px;\">\r\n                Total\r\n            </td>\r\n            <td>\r\n                {% for vendor in vendors %}\r\n                    <td colspan=\"2\" style=\"border: 1px solid black; padding: 3px; text-align: right;\">{{ vendor.total_value }}</td>\r\n                {% endfor %}\r\n            </td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n\r\n\r\n  <table style=\"width: 100%; border-collapse: collapse; border: 1px solid black; margin-top: 10px;\">\r\n    <thead>\r\n        <tr>\r\n            <th colspan=\"4\" style=\"border: 1px solid black; padding: 3px; width: 20%;\">Term</th>\r\n            {% for v in vendors %}\r\n            <th style=\"border: 1px solid black; padding: 3px;\">{{ v.vendor_name }}</th>\r\n            {% endfor %}\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        <tr>\r\n            <td colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\">GST</td>\r\n            {% for v in vendors %}\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ v.gst_number }}</td>\r\n            {% endfor %}\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\">Delivery</td>\r\n            {% for v in vendors %}\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ v.delivery_time }}</td>\r\n            {% endfor %}\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\">Payment Terms</td>\r\n            {% for v in vendors %}\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ v.payment_terms }}</td>\r\n            {% endfor %}\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\">Bidder Rank</td>\r\n            {% for v in vendors %}\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ v.biddle_rank }}</td>\r\n            {% endfor %}\r\n        </tr>\r\n        <tr>\r\n            <td colspan=\"4\" style=\"border: 1px solid black; padding: 3px;\">Transport Charges</td>\r\n            {% for v in vendors %}\r\n            <td style=\"border: 1px solid black; padding: 3px;\">{{ v.transportation_charges }}</td>\r\n            {% endfor %}\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n    <table style=\"width: 100%; border-collapse: collapse; border: 1px solid black; margin-top: -1px; height: 60px;\">\r\n        <tr>\r\n            <td style=\"border: 1px solid black;\"></td>\r\n        </tr>\r\n    </table>\r\n    <!-- Signature Section -->\r\n    <table style=\"width: 100%; border-collapse: collapse; border: 1px solid black; margin-top: -1px;\">\r\n        <tr>\r\n            <td style=\"border: 1px solid black; padding: 3px; width: 20%; font-weight: bold;\">Purchase Department</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; width: 20%; font-weight: bold;\">HOD</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; width: 20%; font-weight: bold;\">Materials Manager</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; width: 20%; font-weight: bold;\">Director of Finance</td>\r\n            <td style=\"border: 1px solid black; padding: 3px; width: 20%; font-weight: bold;\">General Manager</td>\r\n        </tr>\r\n    </table>\r\n</div>\r\n\r\n\r\n",
